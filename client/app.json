{
	"expo": {
		"name": "client",
		"slug": "client",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#202020"
		},
		"plugins": [
			[
				"expo-image-picker",
				{
					"photosPermission": "The app accesses your photos to be able to upload a portrait.",
					"cameraPermission": "The app accesses your camera to be able to take a photo and upload as your portrait"
				}
			]
		],
		"assetBundlePatterns": [
			"**/*"
		],
		"ios": {
			"supportsTablet": true,
			"config": {
				"usesNonExemptEncryption": false
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#202020"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"androidNavigationBar": {
			// "backgroundColor": "#000000"
		}
	}
}